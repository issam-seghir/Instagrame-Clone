{
	"name": "instagram-clone",
	"author": "issam-seghir",
	"private": true,
	"version": "1.0.0",
	"license": "MIT",
	"type": "module",
	"browserslist": [
		"defaults and supports es6-module",
		"maintained node versions"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"clean": "rm -rf dist test",
		"sass:watch": "sass src/scss/main.scss css/main.css --watch --no-source-map --error-css",
		"lint:js": "npx eslint \"**/*.{html,js,ts,jsx,tsx}\"",
		"lint:js:fix": "npm run lint:js -- --fix",
		"lint:style": "npx stylelint \"**/**/*.{html,css,less,postcss,scss,astro}\"",
		"lint:style:fix": "npm run lint:style -- --fix",
		"lint": "npm run lint:js && npm run lint:style",
		"lint:fix": "npm run lint -- --fix",
		"type-check": "tsc --noEmit"
	},
	"devDependencies": {
		"@types/date-fns": "^2.6.0",
		"@types/eslint": "^8.56.0",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"@vitejs/plugin-react": "^4.2.1",
		"eslint": "^8.55.0",
		"eslint-config-mantine": "^3.1.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-compat": "^4.1.4",
		"eslint-plugin-html": "^7.1.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsonc": "^2.9.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-perfectionist": "^2.5.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"eslint-plugin-security": "^1.7.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-sonarjs": "^0.20.0",
		"eslint-plugin-unicorn": "^48.0.1",
		"gerillass": "^1.3.1",
		"postcss-html": "^1.5.0",
		"postcss-inline-svg": "^6.0.0",
		"postcss-markdown": "^1.2.0",
		"postcss-preset-env": "^9.1.3",
		"postcss-preset-mantine": "^1.12.1",
		"sass": "^1.69.4",
		"stylelint": "^15.10.3",
		"stylelint-config-clean-order": "^5.2.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-config-standard-scss": "^10.0.0",
		"stylelint-high-performance-animation": "^1.9.0",
		"typescript": "^5.2.2",
		"vite": "^5.0.12",
		"vite-aliases": "^0.11.3",
		"vite-plugin-svgr": "^4.2.0"
	},
	"dependencies": {
		"@mantine/core": "^7.3.2",
		"@mantine/hooks": "^7.3.2",
		"@reduxjs/toolkit": "^2.0.1",
		"@uidotdev/usehooks": "^2.4.1",
		"date-fns": "^3.0.5",
		"framer-motion": "^10.16.16",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-helmet-async": "^2.0.3",
		"react-redux": "^9.0.4",
		"react-router-dom": "^6.21.1",
		"redux": "^5.0.0",
		"rfs": "^10.0.0"
	}
}
